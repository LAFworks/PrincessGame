{
    switch(page)
    {
        case 0:
        {
            if mpos == 1
            {
                page = 1;
                mpos = 1;
            }
            else if mpos == 2
            {
                page = 2;
                mpos = 1;
            }
            delay = true;
            alarm[0] = 10;
            break;
        }
        case 1:
        {
            if mpos == 1
            {
                show_debug_message("This is a Sword");
